{
  "version": 2,
  "builds": [
    {
      "src": "sigueai-web/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "sigueai-api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "sigueai-api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "sigueai-web/dist/$1"
    }
  ]
}
