{
  "version": 2,
  "builds": [
    { "src": "sigueai-api/api/*.js", "use": "@vercel/node" },
    { "src": "sigueai-web/package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/sigueai-api/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/sigueai-web/$1",
      "continue": true
    }
  ]
}
